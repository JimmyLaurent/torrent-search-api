{
  "name": "torrent-search-api",
  "version": "2.0.3",
  "description":
    "Yet another node torrent scraper based on x-ray. (Support iptorrents, torrentleech, torrent9, Yyggtorrent, ThePriatebay, torrentz2, 1337x, KickassTorrent, Rarbg, TorrentProject, Extratorrent)",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest watch",
    "lint": "eslint ."
  },
  "keywords": [
    "torrent",
    "scraper",
    "crawler",
    "node",
    "api",
    "yggtorrent",
    "torrentz2",
    "torrentleech",
    "iptorrents",
    "1337x",
    "thepiratebay",
    "tpb",
    "kickasstorrents",
    "rarbg",
    "torrentproject"
  ],
  "author": "Jimmy Laurent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/JimmyLaurent/torrent-search-api.git"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "cloudscraper": "^1.5.0",
    "lodash": "^4.17.4",
    "request": "^2.79.0",
    "string-format": "^0.5.0",
    "x-ray-scraper": "^3.0.5"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "eslint": "^4.9.0",
    "nock": "^9.1.6",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "install": "^0.10.4",
    "jest": "^22.4.0",
    "jest-junit": "^3.6.0",
    "lint-staged": "^6.1.1",
    "prettier": "^1.10.2"
  },
  "jest": {
    "testResultsProcessor": "jest-junit",
    "testEnvironment": "node"
  },
  "jest-junit": {
    "output": "./test-results/junit.xml"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "parser": "flow",
    "semi": true,
    "tabWidth": 2
  }
}
